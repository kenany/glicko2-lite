{
  "name": "glicko2-lite",
  "version": "4.0.0",
  "description": "Barebones Glicko-2 implementation",
  "keywords": [
    "statistics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenany/glicko2-lite.git"
  },
  "license": "MIT",
  "author": "Kenan Yildirim <kenan@kenany.me> (https://kenany.me/)",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": "20 || 22 || >=24"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "LICENSE.txt"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
